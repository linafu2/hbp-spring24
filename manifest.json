{
    "manifest_version": 3,
    "name": "Productivity Under the Sea",
    "description": "Extension to facilitate productivity online",
    "version": "1.0",
    "permissions": ["storage", "tabs", "activeTab"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "logo.png"
    },
    "content_scripts": [
      {
        "js": ["content.js"],
        "css" : ["styles.css"],
        "matches": [
          "<all_urls>"
        ]
      }
    ]
  }
  